swiper{
    background-color: yellow;
    height: 30vh;
}
.item1{
    background: orange;

}
.item2{
    background: blue;
    
}
.item3{
    background: greenyellow;
    
}
swiper-item>image{
    width: 100%;
    height: 100%;
}

.grid{
    width: 100%;
    height: 70vh;
    /* background: yellow; */
    display: flex;
    flex-direction: column;
}
.row{
    /* border: solid red 1rpx; */
    flex-grow: 1;
    display: flex;
}
.cell{
    flex-grow: 1;
    /* border: solid red 1rpx; */
    display: flex;
   
    justify-content: center;
    align-items: center;
}
.cell>view{
    width: 25vw;
    height: 25vw;
    background:pink;
    border-radius: 12.5vw;
    display: flex;
    box-shadow:#aaa 0rpx 0rpx 40rpx;
   
    justify-content: center;
    align-items: center;
}
.cell>view>image{
    width: 18vw;
    height: 18vw;

}